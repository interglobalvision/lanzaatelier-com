@font-face {
  font-family: 'Maison Neue Mono';
  src: url('fonts/MaisonNeue-MonoItalic.eot');
  src: url('fonts/MaisonNeue-MonoItalic.eot?#iefix') format('embedded-opentype'),
    url('fonts/MaisonNeue-MonoItalic.woff2') format('woff2'),
    url('fonts/MaisonNeue-MonoItalic.woff') format('woff'),
    url('fonts/MaisonNeue-MonoItalic.ttf') format('truetype'),
    url('fonts/MaisonNeue-MonoItalic.svg#MaisonNeue-MonoItalic') format('svg');
  font-weight: normal;
  font-style: italic;
}

